---

# https://ansible-lint.readthedocs.io/

enable_list:
  - empty-string-compare
  - no-log-password
  - no-same-owner

skip_list:
  - command-instead-of-module
  - command-instead-of-shell
  - latest[git]
  - line[length]
  - meta-no-info
  - meta-no-tags
  - name[casing]
  - name[template]
  - no-changed-when
  - no-handler
  - risky-file-permissions
  - risky-shell-pipe
  - var-naming[no-role-prefix]
  - yaml[line-length]
  - yaml[truthy]
