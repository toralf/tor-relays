---

# https://ansible-lint.readthedocs.io/

enable_list:
  - no-log-password
  - no-same-owner

skip_list:
  - command-instead-of-module
  - latest[git]
  - name[play]
  - name[template]
  - no-handler
  - risky-file-permissions
  - risky-shell-pipe
  - var-naming[no-role-prefix]
  - yaml[truthy]

exclude_paths:
  - site-info.yaml
  - site-setup.yaml
