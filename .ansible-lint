---

# https://ansible-lint.readthedocs.io/

enable_list:
  - empty-string-compare # opt-in
  - no-log-password # opt-in
  - no-same-owner # opt-in

skip_list:
  - latest[git]
  - line[length]
  - meta-no-info
  - meta-no-tags
  - name[template]
  - no-changed-when
  - no-handler
  - risky-file-permissions
  - risky-shell-pipe
  - command-instead-of-shell
  - yaml[truthy]
  - yaml[line-length]
